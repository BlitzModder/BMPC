{
  "name": "BlitzModder",
  "version": "1.2.9",
  "description": "Mod manager for World of Tanks Blitz(PC)",
  "main": "bin/core/core.js",
  "scripts": {
    "teststart": "node_modules\\electron\\dist\\electron.exe bin",
    "start": "npm run start:win64",
    "start:win32": "build -w --ia32 --dir && .\\dist\\win-ia32-unpacked\\BlitzModder",
    "start:win64": "build -w --x64 --dir && .\\dist\\win-unpacked\\BlitzModder",
    "start:mac": "build -m --dir && ./dist/mac/BlitzModder",
    "build": "gulp",
    "pack": "npm run pack:win && npm run pack:mac",
    "pack:win": "npm run build && build -w --ia32 --x64",
    "pack:mac": "npm run build && build -m"
  },
  "author": "S(FV293b)",
  "repository": {
    "type": "git",
    "url": "http://github.com/BlitzModder/BMPC.git"
  },
  "homepage": "https://github.com/BlitzModder/BMPC/wiki",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "fs-extra": "^2.1.2",
    "fstream": "^1.0.11",
    "js-yaml": "^3.8.3",
    "jszip": "^3.1.3",
    "plist": "^2.0.1",
    "promise": "^7.1.1",
    "readdirp": "^2.1.0",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "unzipper": "^0.8.8",
    "vue": "^2.1.3"
  },
  "devDependencies": {
    "coffee-script": "^1.11.1",
    "del": "^2.2.2",
    "electron": "^1.6.2",
    "electron-builder": "^17.0.1",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.2",
    "gulp-coffee": "^2.3.2",
    "gulp-haml": "^0.1.6",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-yaml": "^1.0.1"
  },
  "build": {
    "appId": "xyz.4na.29314.BlitzModderPC",
    "copyright": "(C) S(29314) 2016",
    "electronVersion": "1.6.2",
    "mac": {
      "category": "public.app-category.games"
    },
    "win": {
      "target": "zip"
    },
    "directories": {
      "buildResources": "resources",
      "output": "dist",
      "app": "bin"
    }
  }
}
