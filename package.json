{
  "name": "BlitzModder",
  "version": "1.2.0",
  "description": "Mod manager for World of Tanks Blitz(PC)",
  "main": "bin/core/core.js",
  "scripts": {
    "teststart": "node_modules\\electron\\dist\\electron.exe bin",
    "start": "npm run start:win64",
    "start:win32": "build -w --ia32 --dir && .\\dist\\win-ia32-unpacked\\BlitzModder",
    "start:win64": "build -w --x64 --dir && .\\dist\\win-unpacked\\BlitzModder",
    "start:mac": "build -m --dir",
    "build": "gulp",
    "pack": "npm run pack:win && npm run pack:mac",
    "pack:win": "npm run build && build -w --ia32 --x64",
    "pack:mac": "npm run build && build -m"
  },
  "author": "S(29314)",
  "repository": {
    "type": "git",
    "url": "http://github.com/BlitzModder/BMPC.git"
  },
  "homepage": "https://github.com/BlitzModder/BMPC/wiki",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "4.0.0-alpha.5",
    "fs-extra": "^0.30.0",
    "fstream": "^1.0.11",
    "jszip": "^3.1.3",
    "plist": "^2.0.1",
    "promise": "^7.1.1",
    "readdirp": "^2.1.0",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "unzipper": "^0.8.8",
    "vue": "^2.1.3"
  },
  "devDependencies": {
    "coffee-script": "^1.11.1",
    "del": "^2.2.2",
    "electron": "^1.4.10",
    "electron-builder": "^10.4.1",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.2",
    "gulp-coffee": "^2.3.2",
    "gulp-haml": "^0.1.6",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.2"
  },
  "resolutions": {
    "fs-extra": "0.30.0",
    "globby": "5.0.0",
    "glob": "7.1.1",
    "debug": "2.2.0",
    "minimist": "1.2.0",
    "semver": "5.3.0",
    "mkdirp": "0.5.1",
    "request": "2.75.0",
    "graceful-fs": "4.1.9",
    "nopt": "3.0.6",
    "readable-stream": "2.0.6",
    "xtend": "4.0.1",
    "assert-plus": "0.2.0",
    "path-exists": "1.0.0",
    "camelcase": "2.1.1",
    "strip-bom": "1.0.0",
    "through2": "2.0.1",
    "extend": "2.0.1",
    "vinyl-fs": "0.3.14",
    "vinyl": "0.5.3",
    "gaze": "1.1.2",
    "lodash": "3.10.1",
    "vinyl-sourcemaps-apply": "0.1.4",
    "lodash.clonedeep": "4.5.0",
    "npmlog": "4.0.0"
  },
  "build": {
    "appId": "xyz.4na.29314.BlitzModderPC",
    "copyright": "(C) S(29314) 2016",
    "mac": {
      "category": "public.app-category.games"
    },
    "win": {
      "target": "zip"
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "dist",
    "app": "bin"
  }
}
